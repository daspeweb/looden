{
    "name": "daspeweb/looden",
    "type": "library",
    "description": "Laravel extension to increase developers productivity ",
    "authors": [
        {
            "name": "Guilherme Reis",
            "email": "guilherme.reis@daspeweb.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Looden\\Framework\\": "/"
        },
        "exclude-from-classmap": ["Looden\\Framework\\Company"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Looden\\Framework\\LoodenFrameworkServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/laravel": "5.6.*"
    }
}
